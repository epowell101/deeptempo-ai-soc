{
  "$schema": "https://github.com/anthropics/anthropic-quickstarts/blob/main/mcp/config-schema.json",
  "mcpServers": {
    
    "_comment_infrastructure": "=== INFRASTRUCTURE & DEVELOPMENT (Community Servers) ===",
    
    "github": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-github"],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "${GITHUB_TOKEN}"
      }
    },
    
    "postgres": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-postgres"],
      "env": {
        "POSTGRES_CONNECTION_STRING": "${POSTGRES_URL}"
      }
    },
    
    "_comment_custom": "=== CUSTOM BUSINESS LOGIC ===",
    
    "deeptempo-findings": {
      "command": "python3",
      "args": ["-m", "mcp_servers.deeptempo_findings_server.server"],
      "cwd": "${workspaceFolder}",
      "env": {}
    },
    
    "tempo-flow": {
